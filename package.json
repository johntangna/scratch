{
  "name": "scratch",
  "version": "1.0.0",
  "description": "积木式编程",
  "main": "dist/entry.js",
  "author": "wangsitu",
  "keywords": [
    "calendar",
    "smart",
    "intelligence"
  ],
  "license": "ISC",
  "scripts": {
    "test": "node ./test/test.ts",
    "clear": "rimraf dist",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config ./package/scripts/webpack.config.dev.js",
    "build": "npm run clear && npm run build:umd",
    "build:umd": "cross-env NODE_ENV=production webpack --config ./package/scripts/webpack.config.build.js && npm run copy",
    "build-analyse": "cross-env analyse=true npm run build",
    "build-spa": "cross-env spa=true npm run build",
    "publish-lib": "npm run clear && npm run build && npm publish",
    "copy": "node ./package/scripts/copy.js"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
